<body>
	<div id="wrapper"></div>
	<input type="button" onclick="rd()" value="Click"></input>
	<script type="text/javascript">
		function rd() {
			var myArray = ["Asian", "Thai", "Korean", "Chinese", "Mexican"];

			var wrapper = document.getElementById('wrapper');

   			var elementsToInsert = [];


			for(var i = 0; i < myArray.length; i++) {
			  var radio = document.createElement('input');
			  var label = document.createElement('label');
			  radio.type = 'radio';
			  radio.name = myArray[i];
			  radio.value = myArray[i];

			  label.setAttribute("for", myArray[i]);
			  label.innerHTML = myArray[i];

			  elementsToInsert.push({ label: label, radio: radio });
			}


			while(elementsToInsert.length !== 0) {
			  var randomIndex = Math.floor(Math.random() * elementsToInsert.length);

			 
			  var toInsert = elementsToInsert.splice(randomIndex, 1)[0];

			  wrapper.appendChild(toInsert.label);
			  wrapper.appendChild(toInsert.radio);
			}
		}-->
		<!DOCTYPE html>
<html>
<head>
	<title>StudentsAverage</title>
	<script type="text/javascript">
		var employee=new Object();
		employee.ffname="John Wick";
		employee.id=456;
		employee.fixed=500000;
		employee.variable=100000;
		employee.bonus=50000;

		employee.montlySalary=function () {
			return(employee.fixed+employee.variable)/12;
		};

		employee.yearlySalary=function () {
			return((employee.fixed+employee.variable)*12)+employee.bonus;
		};

		employee.display=function () {
			document.write("Employee name=: "+employee.ffname+"<br>")
			document.write("Employee id: "+employee.id+"<br>");
			var ms=employee.montlySalary();
			var ys=employee.yearlySalary();
			document.write("Monthly Salary: "+ms+"<br>");
			document.write("Yearly Salary: "+ys+"<br>");
		}

		employee.display();
	</script>
</head>

	<script type="text/javascript">

		function show(){
      var students=["rahul","uppu","afdad","ifbai"];
      var text="";
      for(var i=0 ;i<students.length;i++)
      {
      	text+=students[i]+ "<br>";

      }
      document.getElementById("p").innerHTML=text;
}
		

</script>


